  dialogueName: 3-A
  dialogue: "<#\u65C1\u767D>\uFF08\u5468\u672B\uFF0C\u516C\u56ED\uFF09\n<break>\n<#\u6211>
    \uFF08\u73AF\u987E\u56DB\u5468\uFF09\u8FD9\u91CC\u7684\u666F\u8272\u771F\u7684\u597D\u7F8E\uFF0C\u6211\u4E4B\u524D\u90FD\u6CA1\u6709\u53D1\u73B0\u3002\n<break>\n<#\u90BB\u5C45>
    \u6216\u8BB8\u4F60\u53EA\u662F\u88AB\u4E00\u4E9B\u5176\u4ED6\u7684\u4E8B\u60C5\u6240\u56F0\u6270\uFF0C\u6CA1\u6709\u591A\u4F59\u7684\u7CBE\u529B\u53BB\u5173\u6CE8\u8FD9\u4E9B\u7F8E\u597D\u7684\u4E8B\u7269\u3002\n<break>\n<#\u6211>
    \uFF08\u9677\u5165\u6C89\u601D\uFF09\u6211\u6BCF\u5929\u90FD\u5728\u4E0D\u505C\u5730\u5DE5\u4F5C\uFF0C\u5B8C\u5168\u6CA1\u6709\u591A\u4F59\u7684\u65F6\u95F4\u53BB\u5173\u6CE8\u8FD9\u4E9B\u7F8E\u597D\u7684\u4E8B\u7269\u3002\n<break>\n<#\u90BB\u5C45>
    \u6240\u4EE5\u554A\uFF0C\u8FD9\u6B21\u51FA\u6765\u80FD\u8BA9\u4F60\u6682\u65F6\u653E\u4E0B\u90A3\u4E9B\u70E6\u607C\uFF0C\u597D\u597D\u4EAB\u53D7\u8FD9\u4E00\u5207\u7F8E\u597D\u7684\u4E8B\u7269\uFF0C\u6216\u8BB8\u80FD\u591F\u7ED9\u4F60\u5E26\u6765\u4E00\u4E9B\u65B0\u7684\u542F\u53D1\u3002\n<break>\n<#\u6211>
    \uFF08\u770B\u7740\u90BB\u5C45\uFF09\u6216\u8BB8\u4F60\u8BF4\u7684\u5BF9\uFF0C\u6211\u771F\u7684\u9700\u8981\u597D\u597D\u653E\u677E\u4E00\u4E0B\uFF0C\u627E\u56DE\u81EA\u5DF1\u539F\u672C\u7684\u751F\u6D3B\u6001\u5EA6\u3002\n<break>\n<#\u90BB\u5C45>
    \uFF08\u5FAE\u7B11\uFF09\u90A3\u5C31\u5F00\u59CB\u5BFB\u627E\u5427\u3002\u4F60\u53EF\u4EE5\u5C1D\u8BD5\u4ECE\u4E0D\u540C\u7684\u89C6\u89D2\u89C2\u5BDF\u516C\u56ED\u4E2D\u7684\u4E8B\u7269\uFF0C\u6216\u8BB8\u80FD\u591F\u5F97\u5230\u4E00\u4E9B\u610F\u60F3\u4E0D\u5230\u7684\u542F\u53D1\u3002\n<finish>"
