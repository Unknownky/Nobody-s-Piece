  dialogueName: 4-B
  dialogue: "<#\u65C1\u767D>\uFF08\u4ECE\u68A6\u4E2D\u60CA\u9192\uFF0C\u4E00\u79CD\u65E0\u5F62\u7684\u6050\u60E7\u73AF\u7ED5\u7740\u6211\uFF09\n<break>\n<#\u6211>\uFF08\u5750\u8D77\u6765\uFF0C\u53CC\u624B\u6342\u4F4F\u8138\uFF0C\u6DF1\u6DF1\u5730\u5438\u4E86\u4E00\u53E3\u6C14\uFF09\n<break>\n<#\u6211>\uFF08\u4F4E\u58F0\u81EA\u8A00\u81EA\u8BED\uFF09\uFF1A\u201C\u6211\u2026\u2026\u6211\u5230\u5E95\u60F3\u8981\u4EC0\u4E48\u2026\u2026\u201D\n<break>\n<#\u6211>\uFF08\u62FF\u8D77\u6446\u5728\u5E8A\u65C1\u8FB9\u7684\u753B\uFF0C\u9759\u9759\u5730\u770B\u7740\u5B83\uFF09\n<break>\n<#\u6211>\uFF08\u4EFF\u4F5B\u5DF2\u7ECF\u770B\u5230\u4E86\u90A3\u4E2A\u5728\u516C\u56ED\u4E2D\u7ED8\u753B\u7684\u81EA\u5DF1\uFF0C\u90A3\u79CD\u5B81\u9759\u6162\u6162\u629A\u5E73\u4E86\u81EA\u5DF1\u4E0D\u5B89\u7684\u601D\u7EEA\uFF09\n<break>\n<#\u6211>\uFF08\u773C\u795E\u53D8\u5F97\u575A\u5B9A\uFF0C\u6DF1\u6DF1\u5730\u5438\u4E86\u4E00\u53E3\u6C14\uFF09\n<break>\n<#\u6211>\uFF08\u4E0B\u5B9A\u51B3\u5FC3\uFF09\uFF1A\u201C\u6211\u8981\u8F9E\u53BB\u8FD9\u4EFD\u5DE5\u4F5C\uFF0C\u6211\u8981\u53BB\u8FFD\u6C42\u6211\u7684\u68A6\u60F3\uFF0C\u6211\u8981\u53BB\u505A\u6211\u771F\u6B63\u60F3\u505A\u7684\u4E8B\u60C5\uFF01\u201D\n<break>\n<#\u6211>\uFF08\u770B\u7740\u7A97\u5916\u7684\u5929\u7A7A\uFF0C\u5FC3\u4E2D\u5145\u6EE1\u4E86\u671F\u5F85\u548C\u5E0C\u671B\uFF09\n<break>\n<#\u65C1\u767D>\uFF08\u65B0\u7684\u5F00\u59CB\uFF0C\u672A\u77E5\u7684\u65C5\u7A0B\uFF0C\u8FD9\u4E00\u6B21\uFF0C\u6211\u4F1A\u8FFD\u5BFB\u6211\u5FC3\u4E2D\u7684\u68A6\u60F3\uFF09\n<finish>"
