  dialogueName: 4-A
  dialogue: "<#\u65C1\u767D>\uFF08\u68A6\u9B47\uFF09\n<break>\n<#\u68A6\u4E2D\u7684\u62111>\u4F60\u771F\u7684\u60F3\u597D\u4E86\u5417\uFF1F\u4F60\u786E\u5B9A\u8981\u653E\u5F03\u73B0\u5728\u7684\u5DE5\u4F5C\u5417\uFF1F\n<break>\n<#\u68A6\u4E2D\u7684\u62111>\u4F60\u522B\u5FD8\u4E86\u5F53\u521D\u4F60\u5C3D\u4E86\u591A\u5927\u7684\u52AA\u529B\u624D\u5F97\u5230\u8FD9\u4EFD\u5DE5\u4F5C\uFF01\n<break>\n<#\u68A6\u4E2D\u7684\u62112>\u4E3A\u4EC0\u4E48\u4F60\u8FD8\u4E0D\u653E\u5F03\u8FD9\u4EFD\u5DE5\u4F5C\uFF01\u73B0\u5728\u7684\u751F\u6D3B\u5E76\u4E0D\u662F\u4F60\u60F3\u8981\u7684\u4E0D\u662F\u5417\uFF1F\n<break>\n<#\u68A6\u4E2D\u7684\u62111>\u4F60\u8981\u77E5\u9053\uFF0C\u73B0\u5728\u7684\u5DE5\u4F5C\u53EF\u4EE5\u5E26\u7ED9\u4F60\u7A33\u5B9A\u7684\u751F\u6D3B\uFF0C\u81F3\u5C11\u4F60\u4E0D\u7528\u62C5\u5FC3\u660E\u5929\u6CA1\u6709\u94B1\u5403\u996D\u3002\n<break>\n<#\u68A6\u4E2D\u7684\u62112>\u4F60\u4E0D\u80FD\u653E\u5F03\u4F60\u7684\u68A6\u60F3\uFF01\u4F60\u7684\u68A6\u60F3\u662F\u4F60\u751F\u547D\u7684\u4E00\u90E8\u5206\uFF0C\u662F\u4F60\u5BF9\u672A\u6765\u7684\u671F\u5F85\u548C\u5411\u5F80\uFF01\n<break>\n<#\u68A6\u4E2D\u7684\u62112>\u4F60\u4E0D\u80FD\u8BA9\u751F\u6D3B\u7684\u4E0D\u786E\u5B9A\u6027\u538B\u5236\u4F60\u7684\u68A6\u60F3\uFF0C\u66F4\u4E0D\u80FD\u8BA9\u751F\u6D3B\u7684\u538B\u529B\u5265\u593A\u4F60\u7684\u70ED\u60C5\u548C\u60F3\u8C61\u529B\uFF01\n<break>\n<#\u68A6\u4E2D\u7684\u62111>\u6CA1\u4E86\u7A33\u5B9A\u7684\u6536\u5165\u4F60\u600E\u4E48\u7EE7\u7EED\u5728\u8FD9\u751F\u6D3B\u4E0B\u53BB\uFF1F\u96BE\u9053\u4F60\u60F3\u79BB\u5F00\u4F60\u4E00\u76F4\u5411\u5F80\u7684\u57CE\u5E02\u5417\uFF1F\n<break>\n<#\u68A6\u4E2D\u7684\u62112>\u770B\u770B\u4F60\u73B0\u5728\u7684\u751F\u6D3B\u3002\u4F60\u8BE5\u505A\u51FA\u51B3\u5B9A\u4E86\uFF01\n<finish>"
